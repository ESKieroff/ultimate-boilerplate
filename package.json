{
  "name": "ultimate-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "tsup src/index.ts --watch --onSuccess \"env-cmd -f .env nodemon dist/index.js\"",
    "build": "tsup src/index.ts --dts --minify --legacy --format cjs --out-dir dist",
    "start": "env-cmd -f .env node dist/index.js",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "prisma": "npx prisma"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.8",
    "@types/react": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "dotenv": "^16.4.5",
    "env-cmd": "^10.1.0",
    "eslint": "^9.2.0",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "prisma-erd-generator": "^1.11.2",
    "react-refresh": "^0.14.2",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "dependencies": {
    "@prisma/client": "^5.13.0",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "date-fns-tz": "^3.1.3",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "mongodb": "^6.6.0",
    "mongoose": "^8.3.3",
    "pg": "^8.11.5",
    "prisma": "^5.13.0",
    "react": "^18.3.1",
    "tsx": "^4.9.0",
    "zod": "^3.23.6"
  }
}
